<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>NERVESPARKS</title>
    <link rel="stylesheet" href="../Admin-dashboard/assest/css/cdps-4.css">


  </head>
  <body>

  <%-include('./common/header')%>


    <div class="container-fluid new-part">
      <div class="user-box ">
        <div class="user_header">
            <h1>Deal</h1>
        </div>

        <div class="input_box">

            <form action="/deal" method="post"  enctype="multipart/form-data" id="dealershipForm3">

                <input type="text"  class="mt-2" name="dealid" placeholder="Deal Id" >
                <input type="text"  class="mt-3" name="carid" placeholder="Car Id" >
                <input type="file" class="mt-3" name="pics" placeholder="deal info" >

                <button type="submit">submit</button>
            </form>

        </div>

    </div>
    </div>

     <!-- Modal -->
     <div class="modal" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <div class="modal-body">
            <p id="modalMessage"></p>
            <button type="button" class="btn btn-primary fs-14" id="doneButton">Done</button>
          </div>
        </div>
      </div>
    </div>

  <%-include('./common/footer')%>



   <script src="../Admin-dashboard/assest/js/deal.js"></script>

  </body>
</html>